{
  "rules": {
    "no-console": 1,
    "no-debugger": 1,
    "indent": [2, 2, {"SwitchCase": 1}],
    "quotes": [2, "single"],
    "linebreak-style": 0,
    "semi": [2, "never"],
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true,
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ]
}
